{
  "name": "hdrToBabylonEnv",
  "version": "1.1.0",
  "description": "converts .hdr files in directory to .env for babylonjs",
  "repository": "https://github.com/HugoMcPhee/hdr-to-babylon-env",
  "main": "index.mjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc",
    "check-updates": "npx npm-check-updates -i",
    "build-browser": "node scripts/buildBrowser.js",
    "build-main": "node scripts/buildMain.js",
    "build": "npm run build-browser && npm run build-main"
  },
  "bin": "./index.js",
  "keywords": [
    "babylonjs",
    "hdr",
    "env"
  ],
  "author": "Hugo McPhee",
  "license": "MIT",
  "dependencies": {
    "@babylonjs/core": "^7.6.0",
    "@babylonjs/loaders": "^7.6.0",
    "@ffmpeg.wasm/core-mt": "^0.12.0",
    "@ffmpeg.wasm/main": "^0.12.0",
    "@gltf-transform/core": "^3.2.1",
    "@types/puppeteer-core": "^7.0.4",
    "babylonjs": "^7.6.0",
    "chootils": "^0.3.9",
    "chrome-paths": "^1.0.1",
    "delay": "^6.0.0",
    "puppeteer": "^22.8.0",
    "puppeteer-core": "^22.8.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "esbuild": "^0.21.1",
    "typescript": "^5.0.4"
  },
  "prettier": {
    "printWidth": 120
  }
}
